{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": "./src",
    "paths": {
      "@config/*": ["config/*"],
      "@controllers/*": ["controllers/*"],
      "@helpers/*": ["helpers/*"],
      "@models/*": ["models/*"],
      "@services/*": ["services/*"],
      "@routes/*": ["routes/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
